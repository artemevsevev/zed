{
  "features": {
    "inline_completion_provider": "none"
  },
  "theme": {
    "mode": "dark",
    "dark": "Gruvbox Dark",
    "light": "Ros√© Pine Dawn"
  },
  "buffer_font_family": "MonoLisa",
  "buffer_font_features": {
    // "calt": false,
    "liga": false
  },
  "vim_mode": true,
  "relative_line_numbers": true,
  "confirm_quit": true,
  "buffer_font_size": 12,
  "autosave": "on_focus_change",
  "cursor_blink": false,
  "inlay_hints": {
    "enabled": true
  },
  "git": {
    "inline_blame": {
      "enabled": false
    }
  },
  "assistant": {
    "version": "1",
    "enabled": false,
    "provider": {
      "name": "ollama",
      "low_speed_timeout_in_seconds": 30,
      "default_model": {
        "name": "deepseek-coder-v2:latest",
        "keep_alive": "10m",
        "max_tokens": 163840
      }
    }
  }
}
